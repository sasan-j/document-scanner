    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "SmartDoc Video to Image",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "args": [
                "process-smartdoc",
                "../data-doc/SmartDoc/",
                "../data-doc/SmartDocProcessed/",
            ]
        },
        {
            "name": "Generate Training Data New",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "args": [
                "document-data-generator",
                "../data-doc/SmartDocProcessed/",
                "../data-doc/SmartDocNew3/",
            ]
        },
        {
            "name": "Generate Document Training Data",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "args": [
                "document-data-generator",
                "../data-doc/SmartDocProcessed/",
                "../data-doc/document_training/",
            ]
        },
        {
            "name": "Generate Corner Training Data",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "args": [
                "corner-data-generator",
                "../data-doc/SmartDocProcessed/",
                "../data-doc/corner_training/",
            ]
        },
        {
            "name": "Train - Corner",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "args": [
                "train",
                "--model-type",
                "corner",
                "--name",
                "test",
                "--train-dir",
                "../data-doc/SmartDocCornersTrain/",
                "--valid-dir",
                "../data-doc/SmartDocCornersValid/"
            ]
        },
        {
            "name": "Train - Document",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "args": [
                "train",
                "--model-type",
                "document",
                "--name",
                "test",
                "--train-dir",
                "../data-doc/SmartDocDocumentsTrain/",
                "--valid-dir",
                "../data-doc/SmartDocDocumentsValid/"
            ]
        }
    ]
}